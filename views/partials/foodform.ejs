<form action="/food" method="post" enctype='application/x-www-form-urlencoded'>
    <div class="form-row">
        <div class="form-group col">
            <label for="name">Food Name</label>
            <input type="text" name="name" id="name" class="form-control" placeholder="Food Name">
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-3">
            <label for="protein">Email</label>
            <input type="number" name="protein" id="protein" class="form-control" placeholder="Protein">
        </div>
        <div class="form-group col-md-3">
            <label for="carbs">Birthdate</label>
            <input type="number" name="carbs" id="carbs" class="form-control" placeholder="Carbs">
        </div>
        <div class="form-group col-md-3">
            <label for="fats">Birthdate</label>
            <input type="number" name="fats" id="fats" class="form-control" placeholder="Fats">
        </div>
        <div class="form-group col-md-3">
            <label for="time">Time</label>
            <input type="time" name="time" id="time" class="form-control" placeholder="Time">
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Add</button>
</form>